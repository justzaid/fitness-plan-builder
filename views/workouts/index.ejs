<%- include('../partials/head') %>

    <h1>Workout Plans</h1>

    <div class="container">        
        <% if (user) { %>
            <div><h1>Your created list</h1></div>
            <div><button><a class="nav-link" href="/workout-plans/<%= user._id %>/new-plan">Add a Workout plan</a></button></div>  
        <% } %>    
    </div>

    <% workout.forEach(element => { %>
    <li>
     <%= element.name %>
     <%= element.difficultyLevel %>
     <%= element.daysPerWeek %>
     <%= element.timePerWorkout %>
     <%= element.equipmentRequired %>
     <%= element.descritpion %>
    <% }) %>
    </li>
<%- include('../partials/footer') %>